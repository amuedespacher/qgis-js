{
  "name": "qgis-js",
  "version": "0.0.1",
  "description": "QGIS ported to WebAssembly to run it on the web platform",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "main": "dist/qgis.js",
  "types": "dist/qgis.d.ts",
  "scripts": {
    "build": "vite build && rm -rf dist/projects",
    "dev": "vite dev",
    "site": "vite dev",
    "lint": "npm run lint:prettier && npm run lint:clang-format",
    "lint:prettier": "npx prettier . --write",
    "lint:pretty-quick": "pretty-quick --staged",
    "lint:clang-format": "clang-format -i \"--glob=src/**/*.{cpp,hpp}\""
  },
  "dependencies": {
    "browser-fs-access": "0.34.1"
  },
  "devDependencies": {
    "@types/node": "^16.7.13",
    "@types/emscripten": "1.39.7",
    "ol": "^8",
    "typescript": "5.0.4",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2",
    "prettier": "3.0.3",
    "pretty-quick": "3.1.3",
    "clang-format": "1.8.0"
  },
  "peerDependencies": {
    "ol": "^8"
  },
  "peerDependenciesMeta": {
    "ol": {
      "optional": true
    }
  },
  "keywords": [
    "qgis",
    "qgisjs",
    "qgis-js",
    "qgiswasm",
    "qgis-wasm",
    "webassembly",
    "wasm",
    "geo",
    "geospatial"
  ]
}
